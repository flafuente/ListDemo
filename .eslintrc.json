{
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "react-native"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "airbnb-base"
    ],
    "rules": {
      "no-use-before-define": "off",
      "class-methods-use-this": "off",
      "no-underscore-dangle": "off",
      "import/prefer-default-export": "off"
    }
  }
  